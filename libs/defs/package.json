{
	"name": "@expense-tracker/defs",
	"type": "module",
	"private": true,
	"main": "dist/index.cjs",
	"module": "dist/index.mjs",
	"types": "dist/index.d.cts",
	"exports": {
		".": {
			"types": "./dist/index.d.cts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.cjs"
		},
		"./*": {
			"types": "./dist/*.d.cts",
			"import": "./dist/*.mjs",
			"require": "./dist/*.cjs"
		}
	},
	"scripts": {
		"dev": "tsdown --watch",
		"build": "tsdown"
	},
	"devDependencies": {
		"@types/node": "25.1.0",
		"tsdown": "0.20.1",
		"typescript": "5.9.3",
		"zod": "4.3.6"
	},
	"peerDependencies": {
		"zod": "^4.0.0"
	}
}
